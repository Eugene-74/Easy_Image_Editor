[*]

cpp_indent_braces=false  // Do not indent braces.
cpp_indent_multi_line_relative_to=innermost_parenthesis  // Indent multi-line statements relative to the innermost parenthesis.
cpp_indent_within_parentheses=indent  // Indent within parentheses.
cpp_indent_preserve_within_parentheses=false  // Do not preserve indentation within parentheses.
cpp_indent_case_labels=false  // Do not indent case labels.
cpp_indent_case_contents=true  // Indent contents of case statements.
cpp_indent_case_contents_when_block=false  // Do not indent case contents when they are in a block.
cpp_indent_lambda_braces_when_parameter=true  // Indent lambda braces when they are parameters.
cpp_indent_goto_labels=one_left  // Indent goto labels one level to the left.
cpp_indent_preprocessor=leftmost_column  // Place preprocessor directives in the leftmost column.
cpp_indent_access_specifiers=false  // Do not indent access specifiers.
cpp_indent_namespace_contents=true  // Indent contents of namespaces.
cpp_indent_preserve_comments=false  // Do not preserve comment indentation.
cpp_new_line_before_open_brace_control=false  // Ignore new line before opening brace of a control structure.
cpp_new_line_before_open_brace_namespace=false  // Ignore new line before opening brace of a namespace.
cpp_new_line_before_open_brace_type=false  // Ignore new line before opening brace of a type.
cpp_new_line_before_open_brace_function=false  // Ignore new line before opening brace of a function.
cpp_new_line_before_open_brace_block=false  // Ignore new line before opening brace of a block.
cpp_new_line_before_open_brace_lambda=false  // Ignore new line before opening brace of a lambda.
cpp_new_line_scope_braces_on_separate_lines=false  // Do not place scope braces on separate lines.
cpp_new_line_close_brace_same_line_empty_type=false  // Do not place closing brace on the same line for empty types.
cpp_new_line_close_brace_same_line_empty_function=false  // Do not place closing brace on the same line for empty functions.
cpp_new_line_before_catch=true  // Place new line before catch.
cpp_new_line_before_else=true  // Place new line before else.
cpp_new_line_before_while_in_do_while=false  // Do not place new line before while in do-while loops.
cpp_space_before_function_open_parenthesis=remove  // Remove space before function open parenthesis.
cpp_space_within_parameter_list_parentheses=false  // Do not add space within parameter list parentheses.
cpp_space_between_empty_parameter_list_parentheses=false  // Do not add space between empty parameter list parentheses.
cpp_space_after_keywords_in_control_flow_statements=true  // Add space after keywords in control flow statements.
cpp_space_within_control_flow_statement_parentheses=false  // Do not add space within control flow statement parentheses.
cpp_space_before_lambda_open_parenthesis=true  // Do not add space before lambda open parenthesis.
cpp_space_within_cast_parentheses=true  // Do not add space within cast parentheses.
cpp_space_after_cast_close_parenthesis=true  // Do not add space after cast close parenthesis.
cpp_space_within_expression_parentheses=true  // Do not add space within expression parentheses.
cpp_space_before_block_open_brace=true  // Do not add space before block open brace.
cpp_space_between_empty_braces=true  // Do not add space between empty braces.
cpp_space_before_initializer_list_open_brace=true  // Do not add space before initializer list open brace.
cpp_space_within_initializer_list_braces=true  // Add space within initializer list braces.
cpp_space_preserve_in_initializer_list=true  // Preserve space in initializer lists.
cpp_space_before_open_square_bracket=true  // Do not add space before open square bracket.
cpp_space_within_square_brackets=true  // Do not add space within square brackets.
cpp_space_before_empty_square_brackets=true  // Do not add space before empty square brackets.
cpp_space_between_empty_square_brackets=true  // Do not add space between empty square brackets.
cpp_space_group_square_brackets=true  // Group square brackets with spaces.
cpp_space_within_lambda_brackets=true  // Do not add space within lambda brackets.
cpp_space_between_empty_lambda_brackets=true  // Do not add space between empty lambda brackets.
cpp_space_before_comma=true  // Do not add space before comma.
cpp_space_after_comma=true  // Add space after comma.
