cpp_space_remove_around_unary_operator=true  // Remove space around unary operator.
cpp_space_around_binary_operator=insert  // Insert space around binary operator.
cpp_space_around_assignment_operator=insert  // Insert space around assignment operator.
cpp_space_pointer_reference_alignment=left  // Align pointer/reference to the left.
